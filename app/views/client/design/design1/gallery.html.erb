<%= render 'design/design1/layouts/header' %>
<div class="container">
        <div class="row">
            <div class="box">
                <div class="col-lg-12">
                    <hr>
                    <% @gallery.each do |key| %>
                        <img src="<%= key['photos']['data'][0]['source'] if key.has_key?('photos') %>">

                        <%= link_to(key['name'], key['link']) %>

                        <br>
                        <hr>
                    <% end %>
                </div>
            </div>
        </div>

    </div>
    <!-- /.container -->
<%= render 'design/design1/layouts/footer' %>
